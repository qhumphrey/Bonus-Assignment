#Importing libraries
import math

#Defining the add10 function which add 10 to the value returned
def add10(func, par):
    return func(par)+10.0

# Item (a): exp(3)
a = add10(math.exp,3.0)
print('exp(3) + 10 = ', a)



# Item (b): erfc(40)
b = add10(math.erfc,40.0)
print('erfc(40) + 10 =', b)



# Item (c): sin(ax)
A = math.pi/180.0
X = 30.0
ax = A*X
c = add10(math.sin,ax)
print('Item (c) answer is:', c)
